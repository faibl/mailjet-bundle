<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Faibl\MailjetBundle\Command\TestCommand" public="false">
            <argument type="service" id="Faibl\MailjetBundle\Services\MailjetService"/>
            <tag name="console.command" />
        </service>

        <service id="Faibl\MailjetBundle\Services\MailjetService" public="false">
            <argument type="service" id="Mailjet\Client"/>
            <argument type="service" id="Faibl\MailjetBundle\Serializer\Serializer\MailjetMailSerializer"/>
        </service>

        <service id="Faibl\MailjetBundle\Serializer\Serializer\MailjetMailSerializer" public="false">
            <argument type="service" id="Faibl\MailjetBundle\Serializer\Normalizer\MailjetMailNormalizer"/>
            <argument type="service" id="serializer.encoder.json"/>
        </service>

        <service id="Faibl\MailjetBundle\Serializer\Normalizer\MailjetMailNormalizer" public="false">
        </service>

        <service id="Mailjet\Client" public="false">
        </service>
    </services>
</container>
