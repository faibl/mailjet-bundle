{
    "name": "faibl/mailjet-bundle",
    "description": "Symfony bundle for Mailjet integration",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Kandulla",
            "email": "hannes@faibl.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "mailjet/mailjet-apiv3-php": "^1.5",
        "monolog/monolog": "^2.0 || ^3.0",
        "symfony/serializer": "^5.4 || ^6.1 || ^7.3",
        "symfony/console": "^5.4 || ^6.1 || ^7.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4 || ^6.1 || ^7.3",
        "nyholm/symfony-bundle-test": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Faibl\\MailjetBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faibl\\MailjetBundle\\Tests\\": "tests/"
        }
    }
}
